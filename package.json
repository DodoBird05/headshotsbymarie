    "gsap": "^3.14.1",
    "lucide-react": "0.560.0",
    "next": "16.0.8",
    "postcss": "8.5.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwind-merge": "^3.4.0",
